# 生产环境配置模板
# 复制此文件为 .env.production 并填入实际值

NODE_ENV=production

# 数据库连接
DATABASE_URL="postgresql://username:password@localhost:5432/podroom"

# NextAuth配置
NEXTAUTH_SECRET="your-super-secret-key-here"
NEXTAUTH_URL="https://your-domain.com"

# 应用基础URL
NEXT_PUBLIC_BASE_URL="https://your-domain.com"

# Qwen API
QWEN_API_KEY="your-qwen-api-key"

# 阿里云ASR
ALIYUN_ASR_ACCESS_KEY_ID="your-aliyun-asr-key-id"
ALIYUN_ASR_ACCESS_KEY_SECRET="your-aliyun-asr-secret"

# 阿里云OSS
ALIYUN_OSS_ACCESS_KEY_ID="your-aliyun-oss-key-id"
ALIYUN_OSS_ACCESS_KEY_SECRET="your-aliyun-oss-secret"
ALIYUN_OSS_BUCKET="your-bucket-name"
ALIYUN_OSS_REGION="oss-cn-hangzhou"

# 性能优化
NEXT_TELEMETRY_DISABLED=1

